{
  "name": "near-claim-processor",
  "version": "1.0.0",
  "description": "NEAR Protocol claim list processor with Merkle proofs",
  "main": "dist/index.js",
  "scripts": {
    "build": "prisma generate && tsc && cp -r src/generated dist/",
    "postinstall": "prisma generate",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "vitest",
    "test:ci": "vitest --config vitest.config.ci.ts --run",
    "test:local": "vitest --config vitest.config.local.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@prisma/client": "^6.15.0",
    "bn.js": "^5.2.1",
    "csv-parser": "^3.0.0",
    "express": "^4.18.2",
    "keccak": "^3.0.4",
    "prisma": "^6.15.0"
  },
  "devDependencies": {
    "@types/bn.js": "^5.2.0",
    "@types/express": "^4.17.21",
    "@types/keccak": "^3.0.5",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.16",
    "@vitest/coverage-v8": "^1.0.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
